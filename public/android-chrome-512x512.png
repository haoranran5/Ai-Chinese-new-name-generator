<svg width="512" height="512" viewBox="0 0 512 512" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="bg512" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#0a0a1a;stop-opacity:1" />
      <stop offset="25%" style="stop-color:#1a1a2e;stop-opacity:1" />
      <stop offset="75%" style="stop-color:#0f0f23;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#16213e;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="glow512" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#00d4ff;stop-opacity:1" />
      <stop offset="25%" style="stop-color:#0099cc;stop-opacity:1" />
      <stop offset="75%" style="stop-color:#cc0066;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#ff0080;stop-opacity:1" />
    </linearGradient>
    <filter id="neon512" x="-50%" y="-50%" width="200%" height="200%">
      <feGaussianBlur stdDeviation="8" result="coloredBlur"/>
      <feMerge> 
        <feMergeNode in="coloredBlur"/>
        <feMergeNode in="SourceGraphic"/>
      </feMerge>
    </filter>
    <filter id="outerGlow512" x="-50%" y="-50%" width="200%" height="200%">
      <feGaussianBlur stdDeviation="15" result="coloredBlur"/>
      <feMerge> 
        <feMergeNode in="coloredBlur"/>
        <feMergeNode in="SourceGraphic"/>
      </feMerge>
    </filter>
    <filter id="innerGlow512">
      <feGaussianBlur stdDeviation="4" result="coloredBlur"/>
      <feMerge> 
        <feMergeNode in="coloredBlur"/>
        <feMergeNode in="SourceGraphic"/>
      </feMerge>
    </filter>
  </defs>
  
  <!-- Background -->
  <rect width="512" height="512" fill="url(#bg512)" rx="64"/>
  
  <!-- Grid pattern -->
  <defs>
    <pattern id="grid512" width="32" height="32" patternUnits="userSpaceOnUse">
      <path d="M 32 0 L 0 0 0 32" fill="none" stroke="#00d4ff" stroke-width="1" opacity="0.15"/>
      <path d="M 16 0 L 16 32 M 0 16 L 32 16" fill="none" stroke="#00d4ff" stroke-width="0.5" opacity="0.1"/>
    </pattern>
  </defs>
  <rect width="512" height="512" fill="url(#grid512)"/>
  
  <!-- Multiple glow layers for enhanced effect -->
  <g filter="url(#outerGlow512)">
    <g filter="url(#neon512)">
      <!-- "名" character main structure -->
      <g fill="url(#glow512)" stroke="url(#glow512)" stroke-width="8">
        <!-- Top horizontal line -->
        <rect x="86" y="100" width="340" height="24" rx="12"/>
        <!-- Main vertical line -->
        <rect x="244" y="100" width="24" height="312" rx="12"/>
        <!-- Middle horizontal line -->
        <rect x="128" y="230" width="256" height="24" rx="12"/>
        <!-- Bottom left element -->
        <rect x="156" y="324" width="72" height="72" rx="12"/>
        <!-- Bottom right element -->
        <rect x="284" y="324" width="72" height="72" rx="12"/>
      </g>
    </g>
  </g>
  
  <!-- Inner details with separate glow -->
  <g filter="url(#innerGlow512)" opacity="0.8">
    <!-- Additional character details -->
    <rect x="200" y="140" width="112" height="4" rx="2" fill="#00d4ff"/>
    <rect x="200" y="280" width="112" height="4" rx="2" fill="#ff0080"/>
  </g>
  
  <!-- Corner tech accents -->
  <g opacity="0.9">
    <!-- Top left corner -->
    <rect x="40" y="40" width="64" height="8" fill="#00d4ff"/>
    <rect x="40" y="40" width="8" height="64" fill="#00d4ff"/>
    <rect x="32" y="32" width="16" height="2" fill="#00d4ff" opacity="0.6"/>
    <rect x="32" y="32" width="2" height="16" fill="#00d4ff" opacity="0.6"/>
    <polygon points="48,48 64,48 64,52 52,52 52,64 48,64" fill="#00d4ff" opacity="0.4"/>
    
    <!-- Top right corner -->
    <rect x="408" y="40" width="64" height="8" fill="#0099cc"/>
    <rect x="464" y="40" width="8" height="64" fill="#0099cc"/>
    <rect x="464" y="32" width="16" height="2" fill="#0099cc" opacity="0.6"/>
    <rect x="478" y="32" width="2" height="16" fill="#0099cc" opacity="0.6"/>
    
    <!-- Bottom left corner -->
    <rect x="40" y="408" width="64" height="8" fill="#cc0066"/>
    <rect x="40" y="464" width="8" height="64" fill="#cc0066"/>
    <rect x="32" y="464" width="16" height="2" fill="#cc0066" opacity="0.6"/>
    <rect x="32" y="464" width="2" height="16" fill="#cc0066" opacity="0.6"/>
    
    <!-- Bottom right corner -->
    <rect x="408" y="464" width="64" height="8" fill="#ff0080"/>
    <rect x="464" y="408" width="8" height="64" fill="#ff0080"/>
    <rect x="464" y="478" width="16" height="2" fill="#ff0080" opacity="0.6"/>
    <rect x="478" y="464" width="2" height="16" fill="#ff0080" opacity="0.6"/>
    <polygon points="464,464 480,464 480,468 468,468 468,480 464,480" fill="#ff0080" opacity="0.4"/>
  </g>
  
  <!-- Additional tech elements and details -->
  <g opacity="0.6">
    <!-- Corner dots -->
    <circle cx="128" cy="128" r="6" fill="#00d4ff"/>
    <circle cx="384" cy="128" r="6" fill="#0099cc"/>
    <circle cx="128" cy="384" r="6" fill="#cc0066"/>
    <circle cx="384" cy="384" r="6" fill="#ff0080"/>
    
    <!-- Side accent lines -->
    <rect x="64" y="256" width="24" height="2" fill="#00d4ff" opacity="0.8"/>
    <rect x="424" y="256" width="24" height="2" fill="#ff0080" opacity="0.8"/>
    <rect x="256" y="64" width="2" height="24" fill="#0099cc" opacity="0.8"/>
    <rect x="256" y="424" width="2" height="24" fill="#cc0066" opacity="0.8"/>
    
    <!-- Small decorative elements -->
    <rect x="96" y="96" width="8" height="8" rx="2" fill="#00d4ff" opacity="0.3"/>
    <rect x="408" y="96" width="8" height="8" rx="2" fill="#0099cc" opacity="0.3"/>
    <rect x="96" y="408" width="8" height="8" rx="2" fill="#cc0066" opacity="0.3"/>
    <rect x="408" y="408" width="8" height="8" rx="2" fill="#ff0080" opacity="0.3"/>
  </g>
  
  <!-- Subtle inner border -->
  <rect x="8" y="8" width="496" height="496" rx="56" fill="none" stroke="url(#glow512)" stroke-width="1" opacity="0.2"/>
</svg>