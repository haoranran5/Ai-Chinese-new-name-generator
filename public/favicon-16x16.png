<svg width="16" height="16" viewBox="0 0 16 16" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="techBg16" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#0f0f23;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#1a1a2e;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#16213e;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="neonBlue16" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#00d4ff;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#0066ff;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="neonPink16" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#ff0080;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#ff6bb3;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- 背景 -->
  <rect width="16" height="16" rx="3" fill="url(#techBg16)"/>
  
  <!-- 网格效果 -->
  <defs>
    <pattern id="grid16" width="2" height="2" patternUnits="userSpaceOnUse">
      <path d="M 2 0 L 0 0 0 2" fill="none" stroke="#00d4ff" stroke-width="0.1" opacity="0.3"/>
    </pattern>
  </defs>
  <rect width="16" height="16" rx="3" fill="url(#grid16)" opacity="0.4"/>
  
  <!-- 简化的"名"字 -->
  <g transform="translate(8,8)">
    <!-- 夕字部分 -->
    <g stroke="url(#neonBlue16)" fill="none" stroke-width="0.8">
      <line x1="-3" y1="-2" x2="-1" y2="-2"/>
      <path d="M-2.5,-1 Q-3,0 -2,2.5"/>
      <circle cx="-1.5" cy="-0.5" r="0.3" fill="url(#neonBlue16)"/>
    </g>
    
    <!-- 口字部分 -->
    <g stroke="url(#neonPink16)" fill="none" stroke-width="0.6">
      <rect x="0.5" y="-1.5" width="3" height="3.5" rx="0.3"/>
      <line x1="1" y1="-1" x2="3" y2="-1" stroke-width="0.2"/>
      <line x1="1" y1="0" x2="3" y2="0" stroke-width="0.2"/>
      <line x1="1" y1="1" x2="3" y2="1" stroke-width="0.2"/>
    </g>
  </g>
  
  <!-- 边框 -->
  <rect width="16" height="16" rx="3" fill="none" stroke="url(#neonBlue16)" stroke-width="0.3" opacity="0.8"/>
