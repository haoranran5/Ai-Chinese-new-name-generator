<svg width="32" height="32" viewBox="0 0 32 32" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="bg32" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#0a0a1a;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#1a1a2e;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="glow32" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#00d4ff;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#ff0080;stop-opacity:1" />
    </linearGradient>
    <filter id="neon32">
      <feGaussianBlur stdDeviation="1" result="coloredBlur"/>
      <feMerge> 
        <feMergeNode in="coloredBlur"/>
        <feMergeNode in="SourceGraphic"/>
      </feMerge>
    </filter>
  </defs>
  
  <!-- Background -->
  <rect width="32" height="32" fill="url(#bg32)" rx="4"/>
  
  <!-- Grid pattern -->
  <defs>
    <pattern id="grid32" width="4" height="4" patternUnits="userSpaceOnUse">
      <path d="M 4 0 L 0 0 0 4" fill="none" stroke="#00d4ff" stroke-width="0.2" opacity="0.3"/>
    </pattern>
  </defs>
  <rect width="32" height="32" fill="url(#grid32)"/>
  
  <!-- "名" character with glow -->
  <g fill="url(#glow32)" stroke="url(#glow32)" stroke-width="1" filter="url(#neon32)">
    <!-- Top horizontal line -->
    <rect x="6" y="6" width="20" height="2" rx="1"/>
    <!-- Vertical line -->
    <rect x="15" y="6" width="2" height="20" rx="1"/>
    <!-- Middle horizontal -->
    <rect x="8" y="14" width="16" height="2" rx="1"/>
    <!-- Bottom left element -->
    <rect x="10" y="20" width="4" height="4" rx="1"/>
    <!-- Bottom right element -->
    <rect x="18" y="20" width="4" height="4" rx="1"/>
  </g>
  
  <!-- Corner accents -->
  <rect x="2" y="2" width="4" height="1" fill="#00d4ff" opacity="0.8"/>
  <rect x="2" y="2" width="1" height="4" fill="#00d4ff" opacity="0.8"/>
  <rect x="26" y="29" width="4" height="1" fill="#ff0080" opacity="0.8"/>
  <rect x="29" y="26" width="1" height="4" fill="#ff0080" opacity="0.8"/>
</svg>