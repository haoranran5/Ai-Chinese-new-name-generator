<svg width="32" height="32" viewBox="0 0 32 32" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="techBg32" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#0f0f23;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#1a1a2e;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#16213e;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="neonBlue32" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#00d4ff;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#0099ff;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#0066ff;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="neonPink32" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#ff0080;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#ff3399;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#ff6bb3;stop-opacity:1" />
    </linearGradient>
    <filter id="glow32">
      <feGaussianBlur stdDeviation="1" result="coloredBlur"/>
      <feMerge> 
        <feMergeNode in="coloredBlur"/>
        <feMergeNode in="SourceGraphic"/>
      </feMerge>
    </filter>
  </defs>
  
  <!-- 背景 -->
  <rect width="32" height="32" rx="6" fill="url(#techBg32)"/>
  
  <!-- 网格效果 -->
  <defs>
    <pattern id="grid32" width="4" height="4" patternUnits="userSpaceOnUse">
      <path d="M 4 0 L 0 0 0 4" fill="none" stroke="#00d4ff" stroke-width="0.2" opacity="0.3"/>
    </pattern>
  </defs>
  <rect width="32" height="32" rx="6" fill="url(#grid32)" opacity="0.4"/>
  
  <!-- 外层光环 -->
  <circle cx="16" cy="16" r="12" fill="none" stroke="url(#neonBlue32)" stroke-width="0.5" opacity="0.6"/>
  
  <!-- "名"字设计 -->
  <g transform="translate(16,16)">
    <!-- 夕字部分 -->
    <g stroke="url(#neonBlue32)" fill="none" stroke-width="1.5" filter="url(#glow32)">
      <line x1="-6" y1="-4" x2="-2" y2="-4"/>
      <path d="M-5,-2 Q-6,0 -4,5"/>
      <circle cx="-3" cy="-1" r="0.6" fill="url(#neonBlue32)"/>
    </g>
    
    <!-- 口字部分 -->
    <g stroke="url(#neonPink32)" fill="none" stroke-width="1.2" filter="url(#glow32)">
      <rect x="1" y="-3" width="6" height="7" rx="1"/>
      <line x1="2" y1="-2" x2="6" y2="-2" stroke-width="0.4"/>
      <line x1="2" y1="0" x2="6" y2="0" stroke-width="0.4"/>
      <line x1="2" y1="2" x2="6" y2="2" stroke-width="0.4"/>
    </g>
    
    <!-- 科技装饰 -->
    <g stroke="url(#neonBlue32)" fill="none" stroke-width="0.5" opacity="0.7">
      <path d="M-8,-8 L-10,-8 L-10,-6"/>
      <path d="M8,-8 L10,-8 L10,-6"/>
      <path d="M-8,8 L-10,8 L-10,6"/>
      <path d="M8,8 L10,8 L10,6"/>
    </g>
  </g>
  
  <!-- 边框 -->
  <rect width="32" height="32" rx="6" fill="none" stroke="url(#neonBlue32)" stroke-width="0.5" opacity="0.8"/>
</svg>